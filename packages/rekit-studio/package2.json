{
  "name": "rekit-studio",
  "version": "3.0.0",
  "description": "Web IDE for Rekit projects.",
  "keywords": [
    "rekit",
    "react",
    "redux",
    "react-router"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/supnate/rekit.git"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "babel-preset-stage-0"
    ],
    "plugins": [
      "react-hot-loader/babel",
      "transform-decorators-legacy",
      "lodash",
      [
        "import",
        {
          "libraryName": "antd"
        }
      ],
      [
        "module-resolver",
        {
          "alias": {
            "src": "./src"
          }
        }
      ]
    ],
    "env": {
      "dev": {
        "plugins": []
      },
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "include": [
      "index.js",
      "src/**/*.js",
      "tools/lib/*.js"
    ],
    "exclude": [
      "tools/**/templates",
      ".tmp"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": false,
    "sourceMap": false,
    "instrument": true
  },
  "rekit": {
    "version": "2.0.3",
    "devPort": 6078,
    "buildPort": 6079,
    "plugins": [
      "/Users/pwang7/workspace/rekit/packages/rekit-studio/src/features/plugin-cra"
    ],
    "css": "less"
  },
  "theme": {},
  "license": "MIT",
  "scripts": {
    "start": "node ./tools/server.js",
    "build": "node ./tools/build.js",
    "npm:publish": "node ./tools/npm_publish.js",
    "test": "node ./tools/run_test.js",
    "build:test": "node ./tools/server.js -m build"
  },
  "files": [
    "dist",
    "middleware"
  ],
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.3",
    "lodash": "^4.17.10",
    "package-json": "^4.0.1",
    "prettier": "^1.12.1",
    "shelljs": "^0.8.2",
    "socket.io": "^2.0.1",
    "watchpack": "^1.3.1"
  },
  "devDependencies": {
    "ansi-to-html": "^0.6.4",
    "antd": "3.1.3",
    "argparse": "^1.0.10",
    "axios": "^0.17.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chai": "^4.1.2",
    "compression": "^1.7.2",
    "copy-webpack-plugin": "^4.3.1",
    "core-decorators": "^0.20.0",
    "css-loader": "^0.28.11",
    "d3": "^5.3.0",
    "d3-tip": "^0.7.1",
    "dom-scroll-into-view": "^1.2.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "estraverse": "^4.2.0",
    "estraverse-fb": "^1.3.1",
